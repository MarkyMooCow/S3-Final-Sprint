<h1> Here are your PGAdmin results for: <%=input %>  </h1>
<form action="/dataPG" method="GET">
    <button type="submit">Return</button>
</form>
<% info.forEach((data) => { %>
<% if (data.game_title.toUpperCase() === input.toUpperCase()) { %>
<p>
    <div><%=data.game_title %></div>
    <body>
        <b>Developer:</b> <%=data.developer %> |
        <b>Main Character:</b> <%=data.main_character %> |
        <b>Description:</b> <%=data.description %>
    </body>
    <%}})%>
</p>
